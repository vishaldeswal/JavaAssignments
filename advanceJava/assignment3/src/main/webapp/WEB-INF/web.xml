<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
	<display-name>Archetype Created Web Application</display-name>

	<servlet>
		<servlet-name>Login</servlet-name>
		<servlet-class>com.nagarro.login.Login</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>AddProduct</servlet-name>
		<display-name>AddProduct</display-name>
		<description></description>
		<servlet-class>com.nagarro.services.AddProduct</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>DeleteData</servlet-name>
		<display-name>DeleteData</display-name>
		<description></description>
		<servlet-class>com.nagarro.services.DeleteData</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>EditData</servlet-name>
		<display-name>EditData</display-name>
		<description></description>
		<servlet-class>com.nagarro.services.EditData</servlet-class>
	</servlet>

	<servlet>
		<servlet-name>AddUser</servlet-name>
		<display-name>AddUser</display-name>
		<description></description>
		<servlet-class>com.nagarro.services.AddUser</servlet-class>
	</servlet>


	<servlet-mapping>
		<servlet-name>Login</servlet-name>
		<url-pattern>/login</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>AddProduct</servlet-name>
		<url-pattern>/AddProduct</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>DeleteData</servlet-name>
		<url-pattern>/DeleteData</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>EditData</servlet-name>
		<url-pattern>/EditData</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>AddUser</servlet-name>
		<url-pattern>/AddUser</url-pattern>

	</servlet-mapping>





</web-app>


